
<table id="user-data" class="table table-responsive table-bordered table-striped panel-heading custom-table" style="padding: 3px; margin-left: 8px; margin-top: 5px; padding-right: 10px;">
    <thead>
        <tr>
            @*<th><input type="checkbox" id="checkall" /></th>*@
            <th>User Name</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Active</th>
            <th>Join Date</th>
            <th style="text-align: center">Edit</th>
            <th style="text-align: center">Delete</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: users">
        <tr>
            <td data-bind="text: UserName"></td>
            <td data-bind="text: FirstName"></td>
            <td data-bind="text: LastName"></td>
            <td data-bind="text: EmailConfirmed"></td>
            <td data-bind="text: CreateDate"></td>
            
            <td style="text-align: center">
                <p data-placement="top" data-toggle="tooltip" title="Edit"><span class="btn btn-success btn-xs" data-title="Details" data-toggle="modal" data-target="#edit"><span class="glyphicon glyphicon-pencil"></span></span></p>
            </td>
            <td style="text-align: center">
                <p data-placement="top" data-toggle="tooltip" title="Delete"><span class="btn btn-warning btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete"><span class="glyphicon glyphicon-trash"></span></span></p>
            </td>
        </tr>
    </tbody>
</table>
