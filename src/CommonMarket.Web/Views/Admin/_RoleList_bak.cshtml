    

    <table id="role-data" class="table table-responsive table-bordered table-striped panel-heading custom-table" style="padding: 3px; margin-left: 8px; margin-top: 5px; padding-right: 10px;">
    <thead>
        <tr>
            @*<th><input type="checkbox" id="checkall" /></th>*@
            <th>Role Name</th>
            <th>Role Descriptioin</th>

            <th style="text-align: center">Edit</th>
            <th style="text-align: center">Manage</th>
            <th style="text-align: center">Delete</th>
        </tr>
    </thead>
    <tbody data-bind="foreach: roles">

        <tr>
            <td id="r_name" data-bind="text: Name"></td>
            <td id="r_desc" data-bind="text: Description"></td>

            <td style="text-align: center">
                <p data-placement="top" data-toggle="tooltip" title="Edit"><span class="btn btn-success btn-xs" data-title="Edit" data-toggle="modal" data-target="#eId"><span class="glyphicon glyphicon-file"></span></span></p>
            </td>
            <td style="text-align: center">
                <p data-placement="top" data-toggle="tooltip" title="Manage"><span class="btn btn-primary btn-xs" data-title="Manage" data-toggle="modal" data-target="#mId"><span class="glyphicon glyphicon-tasks"></span></span></p>
            </td>
            <td style="text-align: center">
                <p data-placement="top" data-toggle="tooltip" title="Delete"><span class="btn btn-warning btn-xs" data-title="Delete" data-toggle="modal" data-target="#dId"><span class="glyphicon glyphicon-remove"></span></span></p>
            </td>




            <!-- Modal Dialogs - EDIT-->

            
            
        </tr>


    </tbody>

</table>



